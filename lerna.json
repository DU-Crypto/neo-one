{
  "lerna": "2.8.0",
  "packages": [
    "packages/*",
    "packages/*/template/*"
  ],
  "version": "independent",
  "npmClient": "yarn",
  "npmClientArgs": [
    "--ignore-engines"
  ],
  "useWorkspaces": true,
  "commands": {
    "publish": {
      "npmClientArgs": [
        "--access public"
      ]
    }
  }
}
